set terminal postscript eps color solid "Helvetica,22"

set auto
set xlabel "Longitudinal Distance, m"
set ylabel "Depth, m"
set format y "%.1f"
set pointsize 1.0
set key below


plot "<tail -106 profile1.out" using ($4*0.3048):(($5-$13)*0.3048) title "Steady State" with points lt 7, \
      "@SRC@/point.dat" using ($3*0.3048):($10*0.3048) title "Analytic Solution" with lines lt 3

     
